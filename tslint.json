{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rulesDirectory": [],
  "rules": {
    "prettier": [
      true
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ]
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.*",
      "dist/**/*.*",
      "src/**/*.test.tsx"
    ]
  }
}